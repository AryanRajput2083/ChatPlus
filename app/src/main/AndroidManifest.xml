<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ai.frencel20">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".AllChatActivity"
            android:label="@string/title_activity_all_chat"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".ChatActivity"
            android:label="@string/title_activity_chat"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".HtmlActivity"
            android:label="@string/title_activity_html"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Dictionary"
            android:label="@string/title_activity_dictionary"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MessageActivity" />
        <activity android:name=".Matrix" />
        <activity
            android:name=".Text_ts"
            android:label="@string/title_activity_text_ts"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Keep_notes"
            android:label="@string/title_activity_keep_notes"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".FileManage" />
        <activity android:name=".MainCalc" />
        <activity
            android:name=".MainActivity2"
            android:label="@string/title_activity_main2"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>